{{#layout/-graph-page data
      selectedResultId=selectedResultId
      packageFilter=packageFilter
      scriptHash=scriptHash
    as |page|}}

  {{#page.controls local-class="panel" as |layout|}}
    {{#layout.control-row as |row|}}
      {{#row.label text="Time"}}{{row.timeRange}}{{/row.label}}
    {{/layout.control-row}}
    {{#layout.control-row as |row|}}
      {{#row.label text="Script"}}{{row.scripts}}{{/row.label}}
      {{#row.label text="Package"}}{{row.packages}}{{/row.label}}
    {{/layout.control-row}}
  {{/page.controls}}

  {{page.graphBody
      groupDescriber=groupDescriber
      graph-component-name='graph/-version-comparison'}}
{{/layout/-graph-page}}
